			private function keyboardDeleteHandler(event:KeyboardEvent):void {
				switch (event.keyCode) {
					case Keyboard.DELETE:
						deleteItem(list.selectedItem);
						break;
					case Keyboard.D:
						deleteItem(list.selectedItem);
						break;
					case Keyboard.E:
						editScreenDetail(list.selectedItem);
						break;
					case Keyboard.L:
						openLinkTo(list.selectedItem);
						break;
					case Keyboard.B:
						batchUpdate();
						break;
					
				}
			}
			
			
			
