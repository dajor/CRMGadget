			private function splitSymbol(value:String, s:String, e:String):String{
				if(value.indexOf(s)>-1){
					value = value.substring(value.indexOf(s)+1, value.length);
				}
				if(value.indexOf(e)>-1){
					value = value.substring(0,value.indexOf(e));
				}
				return value;
			}
			
