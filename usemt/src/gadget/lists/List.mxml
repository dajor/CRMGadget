				}
				
				if (entity == 'Activity') {
					list.treeColumn = treeColumn;
					
					// if ParentActivityId is not selected in 'Edit Layout'
					// we add it (invisible) here so we are able to use it later
					if (!ParentActivityIdFound) {
						child = new AdvancedDataGridColumn();
